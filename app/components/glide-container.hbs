<div class="GlideContainer" {{did-insert this.onLoad}}>
  <div class="glide GlideHorizontalGlider">
    <div class="GlideHorizontalGlider__header">Glide js</div>
    <div class="glide__track" data-glide-el="track">
      <ul class="glide__slides">
        <li class="glide__slide">Slide 0</li>
        <li class="glide__slide">Slide 1</li>
        <li class="glide__slide">Slide 2</li>
      </ul>
    </div>
    <div class="glide__arrows" data-glide-el="controls">
      <button type="button" class="glide__arrow glide__arrow--left" data-glide-dir="<">
        prev
      </button>
      <button type="button" class="glide__arrow glide__arrow--right"
        data-glide-dir=">">
        next
      </button>
    </div>
  </div>
  <div class="KeenSlider">
    <div class="KeenSlider__header">Keen-slider js</div>
    <div id="my-keen-slider" class="keen-slider KeenHorizontalGlider">
      <div class="keen-slider__slide number-slide1">1</div>
      <div class="keen-slider__slide number-slide2">2</div>
      <div class="keen-slider__slide number-slide3">3</div>
      <div class="keen-slider__slide number-slide4">4</div>
      <div class="keen-slider__slide number-slide5">5</div>
      <div class="keen-slider__slide number-slide6">6</div>
    </div>
  </div>
</div>
